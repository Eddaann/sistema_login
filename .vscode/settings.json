{
    "files.associations": {
        "*.html": "jinja-html"
    },
    "emmet.includeLanguages": {
        "jinja-html": "html"
    },
    "javascript.validate.enable": false,
    "typescript.validate.enable": false,
    "[html]": {
        "editor.quickSuggestions": {
            "other": true,
            "comments": false,
            "strings": true
        }
    },
    "html.validate.scripts": false,
    "html.validate.styles": false,
    "files.exclude": {
        "**/__pycache__": true,
        "**/*.pyc": true
    }
}
